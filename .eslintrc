{
  "root": true,
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false,
    "sourceType": "module"
  },
  "extends": ["@nuxtjs/eslint-config-typescript", "prettier"],
  "rules": {
    "vue/multi-word-component-names": [
      "error",
      {
        "ignores": ["default"]
      }
    ]
  }
}
